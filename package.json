{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "deploy": "./deploy.sh",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "npm lint:eslint && npm lint:prettier",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier . --check",
    "lintfix": "eslint . --fix && prettier --write --list-different .",
    "docs:dev": "cd docs && npm run start",
    "docs:build": "cd docs && npm run build",
    "docs:serve": "cd docs && npm run serve",
    "build:all": "npm run generate && npm run docs:build",
    "dev:all": "concurrently \"npm run dev\" \"npm run docs:dev\"",
    "install:docs": "cd docs && npm install"
  },
  "dependencies": {
    "@nuxtjs/google-fonts": "^3.2.0",
    "nuxt": "^3.12.2",
    "vue": "latest"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.3.13",
    "concurrently": "^9.1.2",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2",
    "sass": "^1.89.1",
    "typescript": "^5.5.2"
  }
}
